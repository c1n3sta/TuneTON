# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# App Configuration
VITE_API_URL=your_api_url
VITE_API_BASE_URL=your_api_base_url

# Telegram Web App Configuration
# Note: TELEGRAM_BOT_TOKEN should only be set in backend environment (not with VITE_ prefix)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
VITE_TELEGRAM_WEBAPP_PLATFORM=web
VITE_TELEGRAM_WEBAPP_URL=your_telegram_webapp_url

# Jamendo API (for music data)
VITE_JAMENDO_CLIENT_ID=your_jamendo_client_id
VITE_JAMENDO_CLIENT_SECRET=your_jamendo_client_secret
VITE_JAMENDO_REDIRECT_URI=your_jamendo_redirect_uri

# FTP Deployment Configuration for Production Builds
FTP_HOST=your_ftp_host
FTP_USER=your_ftp_user
FTP_PASSWORD=your_ftp_password