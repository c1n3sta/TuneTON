            {
                "name": "Message_Processing",
                "entityType": "Component",
                "observations": [
                    "Uses actor model for message passing",
                    "Supports internal and external messages",
                    "Internal messages: from other contracts, can carry value",
                    "External messages: from outside blockchain, no value transfer",
                    "Message body typically includes opcode for operation type",
                    "Implements role-based access control patterns",
                    "Uses message flags for controlling behavior"
                ]
            },
            {
                "name": "Internal_Messages",
                "entityType": "Message_Type",
                "observations": [
