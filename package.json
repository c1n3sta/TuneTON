{
  "name": "tuneton",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "start:dev": "node scripts/start-dev-servers.js",
    "build": "tsc && vite build",
    "build:analyze": "cross-env ANALYZE=true vite build",
    "build:check": "tsc --noEmit && vite build --mode development",
    "build:prod": "npm run clean && vite build --mode production",
    "build:production": "npm run clean && vite build --mode production && echo 'Build completed successfully!' && echo 'Build artifacts are in the dist/ directory'",
    "build:minimal": "npm run clean:dist && npm run type-check && vite build --mode production",
    "build:dev": "npm run clean:dist && npm run type-check && vite build --mode development",
    "build:development": "npm run clean:dist && npm run type-check && vite build --mode development",
    "deploy": "node scripts/deploy-to-production-improved.js",
    "deploy:main": "node scripts/deploy-with-main-user-basicftp.js",
    "deploy:clean": "node scripts/deploy-with-clean.js",
    "deploy:prod": "npm run build:prod && npm run deploy",
    "deploy:prod:clean": "npm run build:prod && npm run deploy:clean",
    "preview": "vite preview --port 3001",
    "preview:prod": "vite preview --port 3001 --mode production",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,html,css}\"",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:increment-counter": "node scripts/test-increment-counter.js",
    "validate:increment-counter": "node scripts/validate-increment-counter-fix.js",
    "clean": "rimraf dist node_modules/.vite .vite",
    "clean:all": "rimraf node_modules dist .vite .vitest coverage",
    "clean:dist": "rimraf dist",
    "prepare": "husky install",
    "postinstall": "npm run build",
    "pre-commit": "lint-staged",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,html,css}\"",
    "migrate:data": "node scripts/migrate-data-to-db.js"
  },
