    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@tuneton/shared": "file:../../../packages/shared",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "winston": "^3.8.2"
