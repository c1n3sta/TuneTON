{
    "version":  "1.0.0",
    "name":  "@tuneton/track-service",
    "description":  "Track Service for TuneTON - Manages track metadata and search functionality",
    "scripts":  {
                    "lint":  "eslint . --ext .ts",
                    "build":  "tsc",
                    "test":  "jest --detectOpenHandles --forceExit",
                    "start":  "node dist/index.js",
                    "dev":  "ts-node-dev --respawn --transpile-only src/index.ts",
                    "format":  "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
                },
    "dependencies":  {
                         "@tuneton/shared":  "workspace:*",
                         "reflect-metadata":  "^0.2.1",
                         "typeorm":  "^0.3.19",
                         "winston":  "^3.11.0",
                         "express":  "^4.18.2",
                         "helmet":  "^7.1.0",
                         "cors":  "^2.8.5",
                         "morgan":  "^1.10.0",
                         "pg":  "^8.11.3"
                     },
    "devDependencies":  {
                            "prettier":  "^3.1.1",
                            "typescript":  "^5.3.3",
                            "eslint-plugin-prettier":  "^5.1.3",
                            "@types/cors":  "^2.8.17",
                            "eslint":  "^8.56.0",
                            "@types/supertest":  "^6.0.2",
                            "@types/jest":  "^29.5.8",
                            "ts-jest":  "^29.1.1",
                            "@types/node":  "^20.10.5",
                            "@types/morgan":  "^1.9.9",
                            "@typescript-eslint/parser":  "^6.15.0",
                            "@types/express":  "^4.17.21",
                            "eslint-config-prettier":  "^9.1.0",
                            "jest":  "^29.7.0",
                            "ts-node":  "^10.9.1",
                            "supertest":  "^6.3.3",
                            "ts-node-dev":  "^2.0.0",
                            "@typescript-eslint/eslint-plugin":  "^6.15.0"
                        },
    "main":  "dist/index.js"
}
